{
	"name": "invision-community",
	"version": "0.1.0",
	"private": true,
	"devDependencies": {
		"babel-preset-expo": "^8.2.3",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.12.1",
		"enzyme-to-json": "^3.3.5",
		"jest-expo": "^38.0.0",
		"jest-serializer-enzyme": "^1.0.0",
		"jsdom": "^15.0.0",
		"prettier": "1.17.0",
		"react-native-mock-render": "^0.1.3",
		"reactotron-react-native": "^4.0.2",
		"reactotron-redux": "^3.1.2"
	},
	"main": "./node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"eject": "expo eject",
		"android": "expo android",
		"ios": "expo ios",
		"test": "jest",
		"editor": "cd web && npm run editor && cd ..",
		"copy:ips": "cp config/ips/assets/splash.png resources/splash.png && cp config/ips/assets/icon.png resources/icon.png",
		"copy:multi": "cp config/multi/assets/splash.png resources/splash.png && cp config/multi/assets/icon.png resources/icon.png",
		"copy:local": "cp config/local/assets/splash.png resources/splash.png && cp config/local/assets/icon.png resources/icon.png",
		"dev:ips": "npm run copy:ips && expo start -c --config config/ips/app.config.js",
		"dev:multi": "npm run copy:multi && expo start -c --config config/multi/app.config.js",
		"dev:local": "npm run copy:local && expo start -c --config config/local/app.config.js",
		"publish:ips": "npm run copy:ips && expo publish --config config/ips/app.config.js",
		"publish:multi": "npm run copy:multi && expo publish --config config/multi/app.config.js",
		"build:ips:ios": "npm run copy:ips && expo build:ios --config config/ips/app.config.js",
		"build:multi:ios": "npm run copy:multi && expo build:ios --config config/multi/app.config.js",
		"build:ips:android": "npm run copy:ips && expo build:android --config config/ips/app.config.js",
		"build:multi:android": "npm run copy:multi && expo build:android --config config/multi/app.config.js",
		"upload:ips:ios": "expo upload:ios --config config/ips/app.config.js",
		"upload:multi:ios": "expo upload:ios --config config/multi/app.config.js",
		"upload:ips:android": "expo upload:android --track internal --config config/ips/app.config.js",
		"upload:multi:android": "expo upload:android --track internal --config config/multi/app.config.js"
	},
	"jest": {
		"preset": "jest-expo",
		"setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		]
	},
	"dependencies": {
		"Base64": "^1.1.0",
		"apollo-cache-inmemory": "^1.6.3",
		"apollo-client": "^2.6.4",
		"apollo-link": "^1.2.13",
		"apollo-link-error": "^1.1.12",
		"apollo-link-http": "^1.5.16",
		"apollo-link-logger": "^1.2.3",
		"buffer": "^5.2.1",
		"deepmerge": "^4.0.0",
		"expo": "^38.0.8",
		"expo-asset-utils": "^1.2.0",
		"expo-crypto": "~8.2.1",
		"expo-image-manipulator": "~8.2.1",
		"expo-image-picker": "~8.3.0",
		"expo-linear-gradient": "~8.2.1",
		"expo-linking": "^1.0.3",
		"expo-localization": "~8.2.1",
		"expo-notifications": "~0.3.3",
		"expo-permissions": "~9.0.1",
		"expo-random": "~8.2.1",
		"expo-secure-store": "~9.0.1",
		"expo-web-browser": "~8.3.1",
		"fuse.js": "^3.4.5",
		"graphql": "^14.5.4",
		"graphql-tag": "^2.10.1",
		"hoist-non-react-statics": "^3.3.0",
		"intl": "^1.2.5",
		"lodash": "^4.17.11",
		"moment": "^2.24.0",
		"polished": "^3.4.1",
		"prop-types": "^15.7.2",
		"query-string": "^6.8.3",
		"quill": "^1.3.6",
		"react": "16.11.0",
		"react-apollo": "^2.5.7",
		"react-dom": "16.11.0",
		"react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
		"react-native-actionsheet": "^2.4.2",
		"react-native-animatable": "^1.3.2",
		"react-native-copilot": "^2.4.5",
		"react-native-easy-toast": "^1.2.0",
		"react-native-fade-in-image": "^1.5.0",
		"react-native-gesture-handler": "~1.6.0",
		"react-native-image-zoom-viewer": "^2.2.26",
		"react-native-keyboard-accessory": "0.1.9",
		"react-native-keyboard-aware-scroll-view": "^0.9.1",
		"react-native-modal": "^11.3.1",
		"react-native-reanimated": "~1.9.0",
		"react-native-render-html": "^4.2.2",
		"react-native-root-toast": "^3.1.1",
		"react-native-slowlog": "^1.0.2",
		"react-native-svg": "12.1.0",
		"react-native-swipeable": "^0.6.0",
		"react-native-tab-view": "^2.9.0",
		"react-native-webview": "9.4.0",
		"react-navigation": "^3.11.1",
		"react-redux": "^6.0.0",
		"redux": "^4.0.1",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"sentry-expo": "2.1.2",
		"short-number": "^1.0.7",
		"underscore": "^1.9.1",
		"validator": "^11.1.0",
		"react-native-safe-area-context": "~3.0.7",
		"react-native-screens": "~2.9.0"
	}
}
